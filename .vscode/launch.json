    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "gateway",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/gateway-service/src/main.ts",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "sourceMaps": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "ai",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/ai-service/src/main.ts",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "sourceMaps": true
        }
    ]
}